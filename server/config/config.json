{
    "appIdentifier": "live",
    "cors": "*",
    "ffmpegFlags": [
        "scale=w=720:h=1280:force_original_aspect_ratio=decrease",
        "-c:a",
        "aac",
        "-ar",
        "48000",
        "-c:v",
        "libx264",
        "-preset",
        "veryfast",
        "-profile:v",
        "main",
        "-crf",
        "20",
        "-sc_threshold",
        "0",
        "-g",
        "48",
        "-keyint_min",
        "48",
        "-hls_time",
        "6",
        "-hls_list_size",
        "10",
        "-hls_flags",
        "delete_segments",
        "-max_muxing_queue_size",
        "1024",
        "-b:v",
        "2800k",
        "-maxrate",
        "2996k",
        "-bufsize",
        "4200k",
        "-b:a",
        "128k"
      ],
    "mediaFormats": {"hls": true, "dash": true},
    "mediaRoot": "./media",
    "persistentStreamFiles": false,
    "saveVideos": false
}